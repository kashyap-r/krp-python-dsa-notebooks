     "text": [
      "60\n"
     ]
    }
   ],
   "source": [
    "#Lowest common Factor\n",
    " \n",
    "a = 15\n",
    "b = 20\n",
    "c = a\n",
    "d = b\n",
    " \n",
    "while b:\n",
    "    a,b=b,a%b\n",
    " \n",
    "lcm = int( (c*d) * 1.0 / a)\n",
    " \n",
    "print(lcm)"
   ]
  },
